<resources>
	<!-- The name of the app -->
	<string name="app_name">AudioWagon</string>
	<!-- The title of the playback queue (a list of tracks to be played back; this is NOT a user-defined playlist). 
	It does not seem to appear in the car -->
	<string name="queue_title">Kolejka odtwarzania</string>
	<!-- When clicking on the magnifying glass to enter the search mode there are 3 headings shown depending on the
	found items: Tracks/Artists/Albums. If possible do not translate "track" to sth like "song", the audio file could
	also contain an	audiobook or podcast -->
	<string name="search_group_tracks">Utwory</string>
	<string name="search_group_artists">Wykonawcy</string>
	<string name="search_group_albums">Albumy</string>
	<!-- The "browse tree" is the main view when starting the app -->
	<!-- There are 3 categories at the top the user can pick from to select media items: Tracks/Albums/Artists -->
	<string name="browse_tree_category_tracks">Utwory</string>
	<string name="browse_tree_category_artists">Wykonawcy</string>
	<string name="browse_tree_category_albums">Albumy</string>
	<!-- The tracks category contains a "fake" entry, when pressed, all tracks will be played back in shuffled order -->
	<string name="browse_tree_pseudo_track_shuffle_all">Odtwarzaj losowo</string>
	<!-- When "Play all" is pressed, all items for the currently shown album or artist will be played back -->
	<string name="browse_tree_pseudo_play_all">Odtwarzaj wszystko</string>
	<!-- When the metadata inside the audio file does not contain information, the album or artist is shown as
	"Unknown" when browsing -->
	<string name="browse_tree_unknown_album">Nieznany album</string>
	<string name="browse_tree_unknown_artist">Nieznany wykonawca</string>
	<!-- Albums that are marked in the metadata as compilations will show the artist "Various artists" -->
	<string name="browse_tree_various_artists">Składanka</string>
	<!-- Some "fake" entries shown in the main browse view e.g. when no USB stick is connected yet -->
	<string name="browse_tree_no_entries_title">Brak plików dźwiękowych</string>
	<string name="browse_tree_no_usb_drive">Proszę podłączyć dysk USB</string>
	<string name="browse_tree_usb_drive_ejected">Dysk USB jest odłączony lub wystąpił błąd odczytu</string>
	<string name="browse_tree_need_to_agree_legal_title">Nie zaakceptowano wyłączeń odpowiedzialności prawnej</string>
	<string name="browse_tree_need_to_agree_legal_desc">Proszę sprawdzić ustawienia aplikacji</string>
	<!-- We use unicode symbols here to avoid the complex linguistic implications in multiple languages for the
	wordings "artists"/"tracks" together with numbers. No need to translate. -->
	<string name="browse_tree_num_albums_num_tracks" translatable="false">%1$d &#x1F4BF; &#x2B1D; %2$d &#x1D160;</string>
	<!-- These "toasts" are short pop-up messages that might be shown to the user at the top or bottom of the screen -->
	<string name="toast_indexing_started">Rozpoczęto indeksowanie USB…</string>
	<string name="toast_indexing_completed">Zakończono indeksowanie USB</string>
	<!-- If there is no next/previous track in the current playlist that the user can skip forward/backward to -->
	<string name="toast_no_next_track">Brak następnego utworu</string>
	<string name="toast_no_prev_track">Brak poprzedniego utworu</string>
	<string name="toast_eject_completed">Odłączono dysk USB, można teraz wyjąć go z gniazda</string>
	<!-- please translate only the "ERROR:" part in front, keep the variable placeholder as-is -->
	<string name="toast_error">Błąd: %1$s</string>
	<!-- We assume "USB" is the same in all languages -->
	<string name="toast_error_USB" translatable="false">USB: %1$s</string>
	<string name="toast_error_USB_IO" translatable="false">I/O</string>
	<!-- Error messages -->
	<!-- The app needs to receive "audio focus" before it can play back any audio. The app will lose "audio focus"
	for example when a navigation route guidance is being read out or when a phone call is ongoing. -->
	<string name="toast_error_audio_focus_denied">Brak dostępu do audio focus</string>
	<string name="toast_error_USB_init">Błąd połączenia z dyskiem USB</string>
	<string name="toast_error_no_filesystem">Nie znaleziono żadnego obsługiwanego systemu plików</string>
	<string name="toast_error_library_creation_fail">Nie udało się utworzyć biblioteki</string>
	<!-- "state" refers to the state the software is in -->
	<string name="toast_error_invalid_state">Błędny stan aplikacji</string>
	<!-- These error strings will be prefixed with the toast_error above, e.g. "ERROR: Timeout" -->
	<string name="toast_error_IO">Błąd wejścia/wyjścia</string>
	<string name="toast_error_malformed_data">Niepoprawne dane</string>
	<string name="toast_error_timeout">Upłynął limit czasu operacji</string>
	<string name="toast_error_not_supported">Brak obsługi</string>
	<string name="toast_error_no_USB_device">Brak urządzenia USB</string>
	<string name="toast_error_unknown">Nieznany</string>
