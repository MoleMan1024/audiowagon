	<string name="notif_action_play">Odtwarzaj</string>
	<string name="notif_action_pause">Pauza</string>
	<string name="notif_action_stop">Zatrzymaj</string>
	<!-- Names for actions to skip to next or previous track. Usually not shown on screen, we use icons -->
	<string name="notif_action_next">Następny</string>
	<string name="notif_action_prev">Poprzedni</string>
	<!-- Names and descriptions for the notification channels, these may appear in Android settings. -->
	<string name="notif_channel_name">Powiadomienia AudioWagon</string>
	<string name="notif_channel_desc">Kanał powiadomień AudioWagon</string>
	<string name="notif_channel_name_indexing">Powiadomienia indeksowania AudioWagon</string>
	<string name="notif_channel_desc_indexing">Kanał powiadomień indeksowania AudioWagon</string>
	<!-- Notifications will appear e.g. in Polestar 2 in a pull-down drawer at the top of the screen (indicated by an
	 orange line) -->
	<string name="notif_indexing_title">Stan indeksowania USB</string>
	<string name="notif_indexing_text_in_progress">W toku…</string>
