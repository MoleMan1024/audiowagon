	<string name="legal_disclaimer_cancel_btn">Cancel</string>
	<string name="toast_error_no_tracks">No tracks</string>
	<string name="toast_error_not_enough_space_for_log">Not enough space to write log file</string>
	<string name="setting_USB_status_too_many_files_in_dir">Too many files in a directory, please use
